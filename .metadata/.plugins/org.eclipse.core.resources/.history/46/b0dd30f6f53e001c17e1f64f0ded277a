
public class practice02 {
	public static void main(String[] args) {
		int a;
		int b;
		a = 20;
		b = a + 5;
		System.out.println(a);
		System.out.println(b);
		
//		インクリメント・デクリメント
		int c = 100;
		int d = 100;
		c++;
		d--;
		System.out.println(c);
		System.out.println(d);
		
//		変数変換
		float e = 3;
		double f = e;
		System.out.println(e);
		System.out.println(f);
	}
}
