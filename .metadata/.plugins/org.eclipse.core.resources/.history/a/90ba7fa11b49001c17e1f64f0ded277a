package game2;

public class Main {
	public static void main(String[] args) {
		Hero h1 = new Hero();
		h1.name = "ミナト";
		
		Hero h2 = new Hero();
		h2.name = "アサカ";
		
		Wizard w = new Wizard();
		w.name = "スガワラ";
		w.hp = 50;
		
		System.out.println(h1.hp);
		w.heal(h1);
		w.heal(h2);
		w.heal(h2);
	}
}
