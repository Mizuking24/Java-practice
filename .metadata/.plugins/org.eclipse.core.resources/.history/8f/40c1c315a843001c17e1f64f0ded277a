
public class practice05 {
	public static void introduceOneself() {
		String name = "吉田";
		int age = 22;
		double size = 170.0;
		char zodiac = '寅';
		System.out.println("私の名前は" + name + "です。");
		System.out.println("年齢は" + age + "です。");
		System.out.println("身長は" + size + "です。");
		System.out.println("干支は" + zodiac + "です。");
	}
}
