package game;

public class Fool extends Character implements Human {
	public void attck() {
		System.out.println(this.name + "は戦わず遊んでいる");
	}
	
	public void talk() {
		System.out.println(this.name + "は戦わず遊んでいる");
	}
	
	public void watch() {
		System.out.println(this.name + "は戦わず遊んでいる");
	}
	
	public void hear() {
		System.out.println(this.name + "は戦わず遊んでいる");
	}
	
	public void run() {
		System.out.println(this.name + "は逃げ出した");
	}
}
